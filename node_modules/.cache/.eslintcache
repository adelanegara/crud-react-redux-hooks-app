aceholder.typicode.com/photos\",\n      {\n        params: {\n          _limit: 10,\n        },\n      }\n    );\n    users.data.forEach((item) => {\n      const { id, title, url } = item;\n      const payload = {\n        id,\n        title,\n        url,\n      };\n      addPhotos(payload);\n    });\n  };\n\n  useEffect(() => {\n    if (photos.length === 0) {\n      fetchData();\n    }\n  }, []);\n\n  useEffect(() => {\n    setFilterData(photos);\n  }, [photos]);\n\n  const handleSearch = (event) => {\n    let value = event.target.value.toLowerCase();\n    let result = [];\n    result = filterData.filter((data) => {\n      return data.title.search(value) !== -1;\n    });\n    setFilterData(result);\n    if (value === \"\") {\n      setFilterData(photos);\n    }\n  };\n\n  const onDeletePhotos = (id) => {\n    deletePhotos(id);\n    toast.dark(\"Delete successfuly\");\n  };\n\n  return (\n    <div className=\"container\">\n      <div className=\"row d-flex flex-column\">\n        <div className=\"col-md-10 mx-auto my-4\">\n          <div className=\"mb-2 mt-2\">\n            {userData?.name && (\n              <p className=\"text-lg-center\">\n                Welcome{\" \"}\n                <strong className=\"text-success\">{userData.name}! </strong>\n                Hi from{\" \"}\n                <strong className=\"text-secondary\">{userData.location}</strong>\n              </p>\n            )}\n          </div>\n          {userData.role === \"admin\" && (\n            <div className=\"mb-2 mt-2\">\n              <button\n                onClick={() => navigate(\"/add\")}\n                className=\"btn btn-outline-dark mr-1\"\n              >\n                Add Photos\n              </button>\n            </div>\n          )}\n\n          <input\n            type=\"text\"\n            className=\"form-control mb-2 mt-2\"\n            placeholder=\"Search\"\n            onChange={(e) => handleSearch(e)}\n          />\n          <table className=\"table table-hover\">\n            <thead className=\"table-header bg-dark text-white\">\n              <tr>\n                <th scope=\"col\">Id</th>\n                <th scope=\"col\">Title</th>\n                <th scope=\"col\">Photos</th>\n                {userData.role === \"admin\" && <th scope=\"col\">Action</th>}\n              </tr>\n            </thead>\n            <tbody>\n              {filterData.length > 0 ? (\n                filterData.map((photos, id) => (\n                  <tr key={id}>\n                    <td>{id + 1}</td>\n                    <td>{photos.title}</td>\n                    <td>\n                      <img src={photos.url} className=\"photos\" />\n                    </td>\n                    {userData.role === \"admin\" && (\n                      <td className=\"d-flex flex-row\">\n                        <Link\n                          to={`/edit/${photos.id}`}\n                          className=\"btn btn-sm btn-primary mr-1\"\n                        >\n                          Edit\n                        </Link>\n                        <button\n                          type=\"button\"\n                          onClick={() => onDeletePhotos(photos.id)}\n                          className=\"btn btn-sm btn-danger mr-1\"\n                        >\n                          Delete\n                        </button>\n                      </td>\n                    )}\n                  </tr>\n                ))\n              ) : (\n                <tr>\n                  <th>No photos found</th>\n                </tr>\n              )}\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => ({\n  photos: state.photos,\n  userData: state.userData,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  deletePhotos: (id) => {\n    dispatch({ type: \"DELETE_PHOTOS\", payload: id });\n  },\n  addPhotos: (data) => {\n    dispatch({ type: \"ADD_PHOTOS\", payload: data });\n  },\n  updatePhotos: (data) => {\n    dispatch({ type: \"UPDATE_PHOTOS\", payload: data });\n  },\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Home);\n","/Users/adelanegaraputri/crud-bank-copy/src/components/Navbar/index.js",[],[],"/Users/adelanegaraputri/crud-bank-copy/src/PrivateRoutes.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/index.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/reportWebVitals.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/App.js",[],[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/PrivateRoutes.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/components/Login/index.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/components/Navbar/index.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/components/Home/index.js",["114","115","116","117","118","119"],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/components/AddUser/index.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/components/EditUser/index.js",["120"],[],"import React, { useEffect, useState, useContext } from \"react\";\nimport { GlobalContext } from \"../../Context/GlobalState\";\nimport { useParams } from \"react-router\";\nimport { useNavigate } from \"react-router-dom\";\nimport { toast } from \"react-toastify\";\n\nconst EditUser = () => {\n  const { photos, updatePhotos } = useContext(GlobalContext);\n\n  const { id } = useParams();\n  const navigate = useNavigate();\n  const currentPhotos = photos.find((item) => item.id === parseInt(id));\n\n  useEffect(() => {\n    setTitle(currentPhotos.title);\n    setUrl(currentPhotos.url);\n  }, [currentPhotos]);\n\n  const [title, setTitle] = useState(\"\");\n  const [url, setUrl] = useState(\"\");\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n    if (!title || !url) {\n      toast.warning(\"Please fill in all fields!!\");\n    }\n\n    const data = {\n      id: currentPhotos.id,\n      title,\n      url,\n    };\n\n    updatePhotos(data);\n    toast.success(\"Photos updated successfully!!\");\n    navigate(\"/\");\n  };\n\n  return (\n    <div className=\"container\">\n      <div className=\"row d-flex flex-column\">\n        <button\n          className=\"btn btn-dark ml-auto my-5\"\n          onClick={() => navigate(\"/\")}\n        >\n          Go back\n        </button>\n        <div className=\"col-md-6 mx-auto shadow p-5\">\n          {currentPhotos ? (\n            <form onSubmit={handleSubmit}>\n              <div className=\"form-group\">\n                <input\n                  className=\"form-control\"\n                  value={title}\n                  placeholder={\"Title\"}\n                  onChange={(e) => setTitle(e.target.value)}\n                />\n              </div>\n              <div className=\"form-group\">\n                <input\n                  className=\"form-control\"\n                  value={url}\n                  placeholder={\"Url Photos\"}\n                  onChange={(e) => setUrl(e.target.value)}\n                />\n              </div>\n              <div className=\"form-group d-flex align-items-center justify-content-between my-2\">\n                <button type=\"submit\" className=\"btn btn-primary\">\n                  Update Photos\n                </button>\n                <button\n                  type=\"button\"\n                  className=\"btn btn-danger\"\n                  onClick={() => navigate(\"/\")}\n                >\n                  cancel\n                </button>\n              </div>\n            </form>\n          ) : (\n            <h1 className=\"text-center\">No Photos Found</h1>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default EditUser;\n","/Users/adelanegaraputri/crud-react-redux-hooks-app/src/Context/GlobalState.js",[],[],"/Users/adelanegaraputri/crud-react-redux-hooks-app/src/Cont